{
    "manifest_version": 2,
    "name": "Tedy's Extension",
    "version": "1.0",
    "description": "Tutorial extension",
    "icons": {
        "128": "./images/icon128.png",
        "48": "./images/icon48.png",
        "16": "./images/icon16.png"
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "./images/icon16.png",
        "default_title": "Teddy's Extension"
    },
    "permissions": [
        "tabs"
    ]
}